<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gamedb.interfaces.AccountMapper">
    <insert id="insertAccount" parameterType="gamedb.pojo.account.AccountPOJO">
        insert into `account`(username,password,channel_name,device_id,register_id) values (#{username},#{password},#{channelName},#{deviceId},#{registerId})
    </insert>
    <select id="getAccount" resultType="gamedb.pojo.account.AccountPOJO">
        SELECT username,password,channel_name,device_id,register_id,create_time FROM `account` WHERE username=#{username} and channel_name=#{channelName}
    </select>
</mapper>